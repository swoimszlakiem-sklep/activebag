lback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"item features-image col-12 col-lg-3 col-md-6\">\n                <a href=\"https://swoimszlakiem-sklep.pl/product/76712448-plecak-na-laptopa-sportowy\" target=\"_blank\">\n                    <div class=\"item-wrapper\">\n                        <div class=\"item-img\">\n                            <img src=\"@PROJECT_PATH@/assets/images/slimpro%202-704x744.jpg\" alt=\"ACTIVE BACKPACK BLACK\">\n                            <div class=\"item-content\" mbr-if=\"showTitle\">\n                                <h5 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-title\">ACTIVE BLACK</h5>\n                            </div>\n                        </div>\n                    </div>\n                </a>\n            </div>\n            <div class=\"item features-image col-12 col-lg-3 col-md-6\">\n                <a href=\"https://swoimszlakiem-sklep.pl/product/76712447-plecak-na-laptopa-sportowy\" target=\"_blank\">\n                    <div class=\"item-wrapper\">\n                        <div class=\"item-img\">\n                            <img src=\"@PROJECT_PATH@/assets/images/slim%20pro%201-704x744.jpg\" alt=\"ACTIVE BACKPACK GREY\">\n                            <div class=\"item-content\" mbr-if=\"showTitle\">\n                                <h5 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-title\">ACTIVE GREY</h5>\n                            </div>\n                        </div>\n                    </div>\n                </a>\n            </div>\n            \n            \n        </div>\n    </div>\n</section>",
